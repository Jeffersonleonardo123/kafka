<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.21.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <changeSet id="1" author="Jefferson">
        <createTable tableName="shop">
            <column name="id" autoIncrement="true" type="bigint">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="statusEnum" type="varchar(100)">
                <constraints nullable="false" />
            </column>
            <column name="dataCompra" type="date"/>
        </createTable>
    </changeSet>
</databaseChangeLog>